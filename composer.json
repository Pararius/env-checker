{
    "name": "pararius/env-checker",
    "description": "Small tool to test your deployment configuration for missing environment variables",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "padraic/phar-updater": "^1.0",
        "symfony/config": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/finder": "^4.3",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "ext-json": "^1.7",
        "friendsofphp/php-cs-fixer": "^2.15",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^8.2",
        "sensiolabs-de/deptrac-shim": "^0.5.0",
        "symfony/var-dumper": "^4.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Pararius\\EnvChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pararius\\EnvChecker\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/env-checker"]
}
